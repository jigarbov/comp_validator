{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.jig_ccomp.interact_reset": {
			"states": {
				"default": {
					"transitions": [
						{"interacted": "query.property('jig_ccomp:interacted')"}
					]
				},
				"interacted": {
					"on_entry": [
						"/function jig/ccomp/portable/settings",
						"@s jig_ccomp:not_interacted"
					],
					"transitions": [
						{"default": "1"}
					]
				}
			}
		}
	}
}

