{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.jig_ccomp.typing_reset": {
			"states": {
				"default": {
					"transitions": [
						{"typing": "query.property('jig_ccomp:typing')"}
					]
				},
				"typing": {
					"on_entry": [
						"/scoreboard players add jig_ccomp:keyboards_tapped jig_ccomp:jig_computer.addon_stats 1",
						"@s jig_ccomp:not_typing"
					],
					"transitions": [
						{"default": "1"}
					]
				}
			}
		}
	}
}

