{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.jig_ccomp.computer": {
			"states": {
				"default": {
					"transitions": [
						{"keyboard": "query.property('jig_ccomp:typing')"}
					],
					"blend_transition": 0.2
				},
				"keyboard": {
					"animations": ["typing"],
					"transitions": [
						{"default": "q.any_animation_finished"}
					],
					"blend_transition": 0.2
				}
			}
		},
		"controller.animation.jig_ccomp.computer_old.screen": {
			"initial_state": "idle",
			"states": {
				"idle": {
					"animations": ["idle"],
					"transitions": [
						{"erro": "query.property('jig_ccomp:screen') == 1"},
						{"home": "query.property('jig_ccomp:screen') == 2"},
						{"emai": "query.property('jig_ccomp:screen') == 3"},
						{"apps": "query.property('jig_ccomp:screen') == 4"},
						{"code": "query.property('jig_ccomp:screen') == 12"},
						{"bugt": "query.property('jig_ccomp:screen') == 15"},
						{"bugo": "query.property('jig_ccomp:screen') == 16"},
						{"bugs": "query.property('jig_ccomp:screen') == 17"},
						{"bugl": "query.property('jig_ccomp:screen') == 18"}
					],
					"blend_transition": 0.2
				},
				"erro": {
					"animations": ["erro"],
					"transitions": [
						{"idle": "query.property('jig_ccomp:screen') != 1"}
					],
					"blend_transition": 0.2
				},
				"home": {
					"animations": ["home"],
					"transitions": [
						{"idle": "query.property('jig_ccomp:screen') != 2"}
					],
					"blend_transition": 0.2
				},
				"emai": {
					"animations": ["emai"],
					"transitions": [
						{"idle": "query.property('jig_ccomp:screen') != 3"}
					],
					"blend_transition": 0.2
				},
				"apps": {
					"animations": ["apps"],
					"transitions": [
						{"idle": "query.property('jig_ccomp:screen') != 4"}
					],
					"blend_transition": 0.2
				},
				"code": {
					"animations": ["code"],
					"transitions": [
						{"idle": "query.property('jig_ccomp:screen') != 12"}
					],
					"blend_transition": 0.2
				},
				"bugt": {
					"animations": ["bugt"],
					"transitions": [
						{"bugo": "query.property('jig_ccomp:screen') == 16"},
						{"idle": "query.property('jig_ccomp:screen') != 16 && query.property('jig_ccomp:screen') != 15\n"}
					]
				},
				"bugo": {
					"animations": ["bugo"],
					"transitions": [
						{"bugs": "query.property('jig_ccomp:screen') == 17"},
						{"bugl": "query.property('jig_ccomp:screen') == 18"},
						{"idle": "query.property('jig_ccomp:screen') != 16 && query.property('jig_ccomp:screen') != 18 && query.property('jig_ccomp:screen') != 17"}
					]
				},
				"bugs": {
					"animations": ["bugs"],
					"transitions": [
						{"bugo": "query.property('jig_ccomp:screen') == 16"},
						{"idle": "query.property('jig_ccomp:screen') != 16 && query.property('jig_ccomp:screen') != 17"}
					]
				},
				"bugl": {
					"animations": ["bugl"],
					"transitions": [
						{"idle": "query.property('jig_ccomp:screen') != 18"}
					],
					"blend_transition": 0.2
				}
			}
		}
	}
}