{
	"format_version": "1.19.0",
	"animation_controllers": {
		"controller.animation.jig_ccomp.printer": {
			"states": {
				"default": {
					"animations": ["idle"],
					"transitions": [
						{"print": "query.property('jig_ccomp:printing')"}
					],
					"blend_transition": 0.2
				},
				"print": {
					"animations": ["print"],
					"transitions": [
						{"default": "query.any_animation_finished"}
					],
					"blend_transition": 0.2
				}
			}
		}
	}
}